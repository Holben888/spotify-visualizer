extends layout

block content
  main
    .navigation
      form#search
        label(for="song-search") Search for music
        .searchbox
          input#song-search(type="text", placeholder="Stevie Wonder")
          button(type="submit")
            img(src="images/ios-search.svg", alt="search button icon")
      ul#search-results
      h1 Saved Songs
      ul#saved-songs
    canvas#visualizer